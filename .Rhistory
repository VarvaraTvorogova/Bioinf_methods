data <- read.csv('/mnt/Data/MEGA/Research/Experiments/Embryo_count/2020_06_09_R108_108_1_calli_with_embryos.csv')
dat_long <- data %>%
gather("Calli", "Number", -Genotype)
datagus <- subset (dat_long, Genotype=="R-108 GUSOE"| Genotype =="108-1 GUSOE")
ggplot(datagus, aes (x = Genotype, y = Number))+
geom_col(aes(fill = Calli), colour = 'darkslategrey')+
